<!-- Top Breadcrumb -->
<div class="breadcrumb is-small is-white" aria-label="breadcrumbs">
    <ul>
        <li>
            <a asp-area="Lms"
               asp-controller="ClassRoutines"
               asp-action="index"><strong>Class Routine</strong></a>
        </li>
        <li class="is-active"><a href="#" class="has-text-white" aria-current="page"><strong>Index</strong></a></li>
    </ul>
</div>

<!-- Top Level -->
<div class="level">
    <!-- Left side -->
    <div class="level-left">
        <div class="level-item">
            <div class="title has-text-white">
                <strong>My class routine</strong>
            </div>
        </div>
    </div>

    <!-- Right side -->
    <div class="level-right">
        <div class="buttons has-addons is-centered">
            
        </div>
    </div>
</div>

<!-- Table -->
<div class="table-container has-background-info-light px-2 py-2 mb-0">
    <table id="tblData" class="table is-bordered is-striped is-narrow is-hoverable is-fullwidth display compact">
        <thead class="has-background-grey-light">
            <tr>
                <th><abbr title="C:">Class Name</abbr></th>
                <th><abbr title="D:">Day</abbr></th>
