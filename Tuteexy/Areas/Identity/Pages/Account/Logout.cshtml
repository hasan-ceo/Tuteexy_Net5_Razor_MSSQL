@page "/Logout"
@model LogoutModel
@{ ViewData["Title"] = "Log out"; }


<div class="row justify-content-center align-content-center">
    <div class="col-sm-10 col-md-10 p-4">
        <section>
            <h3>
                @ViewData["Title"]
            </h3>
            <h5 class="text-danger">
                You have successfully logged out of the application.
            </h5>
        </section>
    </div>
</div>